{
  "manifest_version": 3,
  "name": "Auth Tabs Sync",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "assets/background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
